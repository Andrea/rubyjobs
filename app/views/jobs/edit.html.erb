
<h1>Edit Job</h1>

<% form_for(@job) do |form| %>
  <%= form.error_messages %>
	<%= hidden_field_tag :key, @job.key %>
  <%= render :partial => 'form_fields', :locals => {:form => form} %>

  <p class='form_control'>
    <%= form.submit "Update" %>
    <%= link_to 'Back', jobs_path %>
  </p>
<% end %>

